{schema knd-basic

{!class Edible Object
    {is Organic Object}
    {inner nutr {c Nutritional Value} {t set}}
}

{!class Nutritional Value
    [!_gloss {ru {t пищевая ценность}}]
    {is Information}
    {ref source {c Authoritative Source} {impl}}
    {num energy}
    {float carbs}
    {float fat}
    {float protein}}

{!class Authoritative Source
    {is Agent}}

{!class USDA
    {is Authoritative Source}}

{!class Fruit
    {is Edible Object}}

}