{gsl::Knowdy Basic
  {knd::Knowdy Learner Service Configuration
      {agent 007}
      {path /var/lib/knowdy/db}
      {schemas /etc/knowdy/schemas}
      {sid AUTH_SERVER_SID}
      {max_objs 500000}

      {delivery ipc:///var/lib/knowdy/delivery/inbox}

      {write_inbox
       {frontend ipc:///var/lib/knowdy/writer_inbox_frontend}
       {backend ipc:///var/lib/knowdy/writer_inbox_backend}
      }

      {publish
       {frontend ipc:///var/lib/knowdy/learner_pub_frontend}
       {backend ipc:///var/lib/knowdy/learner_pub_backend}
      }

}}
