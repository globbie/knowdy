{schema knd

{!class 3D Shape
        [_gloss {ru {t форма объекта}}]
    {is Abstraction}}

{!class 2D Shape
        [_gloss {ru {t двумерная форма объекта}}]
    {is Abstraction}}

{!class Sharp Shape
        [_gloss {ru {t заостренная форма объекта}}]
    {is 3D Shape}}

{!class Round Shape
        [_gloss {ru {t круглая форма объекта}}]
    {is 2D Shape}}


}
